                </cit>
                <!--**ASSIGNED**-->
                <cit xml:id="Ari_Meta_4-1-1003a31-32" source="#Ari_Meta_4-1-1003a31-32.xml">
                    <bibl>
                        <author ref="#p_Aristotle">Arist.</author>
                        <title ref="#w_metaph">Metaph.</title>
                        <citedRange corresp="#ar_met_perseus" target="http://data.perseus.org/citations/urn:cts:greekLit:tlg0086.tlg025.perseus-grc1:4.1003a">Γ, 1, 1003a31-32</citedRange>
